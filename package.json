{
  "name": "cypres-demo-app",
  "version": "1.0.0",
  "description": "Sample application for demo purposes.",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "cypress-open": "npx cypress open --config-file=cypress.json",
    "cypress-run-sanity": "npm run clean-data && npm run sanity && npm run report",
    "sanity": "npx cypress-tags run -e TAGS='@sanity' --config-file=cypress.json",
    "report": "node cypress/support/generate_report.js",
    "clean-data": "npm run clean-report && npm run clean-videos && npm run clean-screenshots && npm run clean-cucumber",
    "clean-report": "rm -rf ./cypress/reports",
    "clean-videos": "rm -rf ./cypress/videos",
    "clean-screenshots": "rm -rf ./cypress/screenshots",
    "clean-cucumber": "rm -rf ./cypress/cucumber-json"
  },
  "author": "Refleqt NV",
  "license": "Private",
  "dependencies": {
    "fs-extra": "^9.1.0",
    "jquery": "^3.5.1",
    "multiple-cucumber-html-reporter": "^1.18.0"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "devDependencies": {
    "cypress": "^11.2.0",
    "cypress-cucumber-preprocessor": "^4.0.1",
    "cypress-downloadfile": "^1.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ""
    }
  }
}
